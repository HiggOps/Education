body {
	font-family: arial;
	align: center;
}
.container {
	width: 1000px;
	height: 100%;
	background-color: #6495ED;
	display: flex;

}
.left {
	float: left;
	width: 40%;
	height: 600px;
	padding: 30px;
	text-align: center;
	display: flex;
}
.right {
	float: right;
	width: 60%;
	height: 600px;
	padding: 30px;
	color: white;
	display: flex;
}
a {
	color: white;
	font-weight: none;
}
	